  "version": "1.0.0",
  "description": "A TS/JS library for doing immutable updates and querying on nested data structures in a way that is composable and powerful.",
  "main": "dist/accessor.js",
  "module": "dist/esm/accessor.js",
  "author": "jethro larson <jethrolarson@users.noreply.github.com> (https://github.com/jethrolarson)",
  "scripts": {
