{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build-cjs",
          "build-esm",
          "build",
          "lint",
          "prepublishOnly",
          "test",
          "test-cover"
        ]
      }
    }
  },
  "targetDefaults": {
    "build-cjs": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build-esm": {
      "outputs": [
        "{projectRoot}/dist/esm"
      ]
    },
    "build": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "prepublishOnly": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  }
}
